<p id="notice"><%= notice %></p>

<h1>Attempt Choices</h1>

<table>
  <thead>
    <tr>
      <th>Attempts</th>
      <th>Questions</th>
      <th>Choices</th>
      <th>User</th>
      <th>Team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attempt_choices.each do |attempt_choice| %>
      <tr>
        <td><%= attempt_choice.attempts %></td>
        <td><%= attempt_choice.questions %></td>
        <td><%= attempt_choice.choices %></td>
        <td><%= attempt_choice.user %></td>
        <td><%= attempt_choice.team %></td>
        <td><%= link_to 'Show', attempt_choice %></td>
        <td><%= link_to 'Edit', edit_attempt_choice_path(attempt_choice) %></td>
        <td><%= link_to 'Destroy', attempt_choice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attempt Choice', new_attempt_choice_path %>
